<div class="container">

           <main>
            <div class="grid-container">
                <div class="grid-x grid-padding-x">
                    <div class="photo cell medium-3 large-3">
                        <img src="./assets/img/tutorimg.jpg" alt="profile" width="100">
                        <button class="photo-btn">+</button>
                        <h5>{{this.name}}</h5>
                    </div>
                    <div class="cell medium-9 large-9 text-left">
                        <div class="grid-container nested">
                            <div class="grid-x">
                                <div class="cell shrink">
                                    <h6>Bio:</h6>
                                </div>
                                <div class="cell auto">
                                    <p>{{this.bio}}</p>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="grid-container nested">
                            <div class="grid-x">
                                <div class="cell shrink">
                                    <h6>Knowledgeable in:</h6>
                                </div>
                                <div class="cell auto">
                                    <ul>
                                        <li>Math</li>

                                    </ul>
                                </div>
                                <div class="cell shrink">
                                    <h6>Rating:</h6>
                                </div>
                                <div class="cell auto">
                                    <p>8.6/10</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr>
                    <div class="cell">
                        <div class="card">
                            <div class="card-section">
                                <div class="calendar">

                                    <header>

                                        <h2>August</h2>

                                        <a class="btn-prev fontawesome-angle-left" href="#"></a>
                                        <a class="btn-next fontawesome-angle-right" href="#"></a>

                                    </header>

                                    <table>

                                        <thead>

                                            <tr>

                                                <td>Mo</td>
                                                <td>Tu</td>
                                                <td>We</td>
                                                <td>Th</td>
                                                <td>Fr</td>
                                                <td>Sa</td>
                                                <td>Su</td>

                                            </tr>

                                        </thead>

                                        <tbody>

                                            <tr>
                                                <td class="prev-month">30</td>
                                                <td class="prev-month">31</td>
                                                <td>1</td>
                                                <td>2</td>
                                                <td>3</td>
                                                <td>4</td>
                                                <td>5</td>
                                            </tr>
                                            <tr>
                                                <td>6</td>
                                                <td>7</td>
                                                <td>8</td>
                                                <td>9</td>
                                                <td>10</td>
                                                <td>11</td>
                                                <td>12</td>
                                            </tr>
                                            <tr>
                                                <td>13</td>
                                                <td class="event">14</td>
                                                <td>15</td>
                                                <td>16</td>
                                                <td>17</td>
                                                <td>18</td>
                                                <td>19</td>
                                            </tr>
                                            <tr>
                                                <td>20</td>
                                                <td>21</td>
                                                <td>22</td>
                                                <td>23</td>
                                                <td>24</td>
                                                <td class="current-day event">25</td>
                                                <td>26</td>
                                            </tr>

                                            <tr>
                                                <td class="event">23</td>
                                                <td>27</td>
                                                <td>28</td>
                                                <td>29</td>
                                                <td>30</td>
                                                <td>31</td>
                                                <td class="next-month">1</td>
                                            </tr>
                                            <tr>
                                                <td class="next-month">2</td>
                                                <td class="next-month">3</td>
                                                <td class="next-month">4</td>
                                                <td class="next-month">5</td>
                                                <td class="next-month">6</td>
                                                <td class="next-month">7</td>
                                                <td class="next-month">8</td>
                                            </tr>

                                        </tbody>

                                    </table>

                                </div> <!-- end calendar -->
                            </div>
                        </div>
                    </div>
                    {{#each message}}  
                    <div class="cell auto">
                        <div class="card">
                            <div class="card-section" id="emailList">
                                <div class="email-list">
                                    <div class="email-list-item email-list-item--unread">
                                        <div class="email-list-actions">
                                            <label class="custom-control custom-checkbox">
                                                <input type="checkbox" class="custom-control-input"><span class="custom-control-label"></span>
                                            </label><a href="#" class="favorite active"><span class="mdi mdi-star"></span></a>
                                        </div>
                                        <div class="email-list-detail"><span class="date float-right"><i class="icon mdi mdi-attachment-alt"></i>
                                                28 Jul</span><span class="from">Penelope Thornton</span>
                                            <p class="msg">{{this.message}}</p>
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    <div class="cell auto">
                        <div class="card" style="height: 400px">
                            <div class="card-section">
                                <p>Flex Space</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>

</div>